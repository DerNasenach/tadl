# ...contains the applications version.
# see csr::ShowAppVersion
dto AppVersion struct{
    # ...is the major incompatible app version.
    Major int32!

    # ...is the minor "just new feature" version.
    Minor int32!

    # ...is the micro bugfix version.
    Micro int32!

    # ...is some unique incrementing play store version.
    Version int64!

    # ...is the applications name.
    Name string!
}